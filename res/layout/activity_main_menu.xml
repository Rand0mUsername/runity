<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginBottom="0dp"
    android:background="#ffffff"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:gravity="center"
    android:ignoreGravity="@drawable/bgr3"
    android:paddingLeft="0dp"
    android:paddingRight="0dp" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1"
            android:src="@drawable/phantom" />

        <fragment
            android:id="@+id/main_map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.8" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1"
            android:src="@drawable/phantom" />
    </LinearLayout>

    <ImageView
        android:id="@+id/background_gray"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="0.33"
        android:scaleType="centerCrop"
        android:src="@drawable/bgr3" />

    <LinearLayout
        android:id="@+id/main_content_container"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/upper_dock"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1"
            android:background="@drawable/dock" >

            <LinearLayout
                android:id="@+id/upper_helper"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.15"
                    android:src="@drawable/phantom" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:src="@drawable/logo_a" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.15"
                    android:src="@drawable/phantom" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mid_dock"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.8"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" >

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.11"
                    android:src="@drawable/phantom" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.8"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/imageView9"
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.0"
                        android:src="@drawable/phantom" />

                    <ImageView
                        android:id="@+id/imageView1"
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.90"
                        android:adjustViewBounds="true"
                        android:src="@drawable/cicrcle" />

                    <ImageView
                        android:id="@+id/imageView10"
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.0"
                        android:src="@drawable/phantom" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.11"
                    android:src="@drawable/phantom" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lower_dock"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1"
            android:background="@drawable/dock"
            android:baselineAligned="false"
            android:clickable="true"
            android:gravity="bottom" >

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.083"
                android:src="@drawable/phantom" />

            <ImageView
                android:id="@+id/main_imageView_addChallenge"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.167"
                android:adjustViewBounds="false"
                android:clickable="true"
                android:maxWidth="@dimen/activity_horizontal_margin"
                android:src="@drawable/add_c_selecter" />

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.167"
                android:src="@drawable/phantom" />

            <ImageView
                android:id="@+id/main_imageView_runity"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_marginBottom="4dp"
                android:layout_marginTop="4dp"
                android:layout_weight="0.167"
                android:clickable="true"
                android:src="@drawable/runity_gray" />

            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.167"
                android:src="@drawable/phantom" />

            <ImageView
                android:id="@+id/main_imageView_routesHub"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.167"
                android:adjustViewBounds="false"
                android:baselineAlignBottom="false"
                android:clickable="true"
                android:src="@drawable/routes_selecter" />

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.083"
                android:src="@drawable/phantom" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/text_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.025"
            android:src="@drawable/phantom" />

        <TextView
            android:id="@+id/main_textView_infoKcal"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.29"
            android:gravity="center"
            android:scrollHorizontally="false"
            android:text="999kcal"
            android:textColor="@color/red"
            android:textSize="@dimen/dp30" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.32"
            android:src="@drawable/phantom" />

        <LinearLayout
            android:id="@+id/param_container"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.27" >

            <TextView
                android:id="@+id/main_textView_infoTime"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="0.5"
                android:gravity="center"
                android:text="0:0:0"
                android:textColor="@color/Red"
                android:textSize="@dimen/dp30" />

            <TextView
                android:id="@+id/main_textView_infoMeters"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="0.5"
                android:gravity="center"
                android:text="156m"
                android:textColor="@color/Red"
                android:textSize="@dimen/dp30" />

        </LinearLayout>

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.04"
            android:src="@drawable/phantom" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/log_out_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.106"
            android:src="@drawable/phantom" />

        <LinearLayout
            android:id="@+id/logout"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.04"
            android:gravity="right" >

            <ImageView
                android:id="@+id/main_imageView_center"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="0.2"
                android:clickable="true"
                android:src="@drawable/center" />

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="0.9"
                android:src="@drawable/phantom" />

            <ImageView
                android:id="@+id/main_imageView_logout"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="0.25"
                android:adjustViewBounds="true"
                android:alpha="5"
                android:clickable="true"
                android:scaleType="centerInside"
                android:src="@drawable/close" />
        </LinearLayout>

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.85"
            android:src="@drawable/phantom" />
    </LinearLayout>

</RelativeLayout>